{{!-- <div class="container"> --}}
<div class="row m-3" style="height: 100vh">
  <div class="col-12 p-5">

    <table class="table">
      <thead class="thead-light">
        <tr>
          <th scope="col">Student ID</th>
          <th scope="col">Class</th>
          <th scope="col">Exam Name</th>
          <th scope="col">Tamil</th>
          <th scope="col">English</th>
          <th scope="col">Maths</th>
          {{!-- <th scope="col">Science</th>
          <th scope="col">Social</th> --}}
        </tr>
      </thead>
      <tbody>
        {{#each marks}}
        <tr>
          <td>{{this.studentId}}</td>
          <td>{{this.course}}</td>
          <td>{{this.exam}}</td>
          <td>{{this.tamil}}</td>
          <td>{{this.english}}</td>
          <td>{{this.maths}}</td>
          {{!-- <td>{{this.science}}</td>
          <td>{{this.social}}</td> --}}
        </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
</div>